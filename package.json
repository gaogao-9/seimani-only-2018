{
  "name": "seimani-only-2018",
  "version": "1.0.0",
  "description": "政剣マニフェスティアオンリーイベント2018公式サイトです",
  "author": "がお <gaotuki@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=develop PORT=2018 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "_generate": "cross-env NODE_ENV=production nuxt generate",
    "generate": "npm run _generate && cp ./generator/.git ./dist/.git && cp ./generator/CNAME ./dist/CNAME",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "element-ui": "^2.1.0",
    "nuxt": "^1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "cross-env": "^5.1.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
